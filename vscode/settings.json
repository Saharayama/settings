{
    "editor.fontFamily": "Ricty",
    "editor.insertSpaces": true,
    "workbench.startupEditor": "none",
    "editor.detectIndentation": true,
    "editor.wordWrap": "on",
    "editor.autoClosingBrackets": "never",
    "extensions.ignoreRecommendations": true,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "always",
    "editor.minimap.enabled": false,
    "editor.fontSize": 16,
    "editor.snippetSuggestions": "inline",
    "editor.renderLineHighlight": "all",
    "editor.quickSuggestionsDelay": 0,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.suggest.localityBonus": true,
    "editor.wordSeparators": "`~!@$%^&*()=+[{]}|;:'\",<>/?.#にをはがのともへでや。、，．・：（）",
    "files.trimTrailingWhitespace": true,
    "[markdown]": {
        "files.trimTrailingWhitespace": false
    },
    "extensions.autoCheckUpdates": false,
    "update.mode": "manual",
    "update.enableWindowsBackgroundUpdates": false,
    "extensions.autoUpdate": false,
    "workbench.settings.editor": "json",
    "workbench.settings.openDefaultKeybindings": false,
    "workbench.colorCustomizations": {
        "editor.selectionHighlightBackground": "#00000000",
        "editor.foreground": "#ffffff",
        "editor.background": "#000000",
        "statusBar.noFolderBackground": "#0000ff00",
        "statusBar.foreground": "#eeeeee",
        "statusBar.background": "#66008000",
        // "editorGroupHeader.tabsBorder": "#ff0000",
        // "statusBar.border": "#ff0000",
        // "panel.border": "#ff0000",
        "terminal.foreground": "#ffffff",
        "terminal.ansiBlack": "#000000",
        "terminal.ansiBlue": "#555555",
        "terminal.ansiBrightBlack": "#ffffff",
        "terminal.ansiBrightBlue": "#9090ff",
        "terminal.ansiBrightCyan": "#40ffff",
        "terminal.ansiBrightGreen": "#40ff40",
        "terminal.ansiBrightMagenta": "#ff40ff",
        "terminal.ansiBrightRed": "#e74856",
        "terminal.ansiBrightWhite": "#ffffff",
        "terminal.ansiBrightYellow": "#ffff00",
        "terminal.ansiCyan": "#00bfbf",
        "terminal.ansiGreen": "#ffffff",
        "terminal.ansiMagenta": "#881798",
        "terminal.ansiRed": "#c50f1f",
        "terminal.selectionBackground": "#ffffff66",
        "terminal.ansiWhite": "#ffffff",
        "terminal.ansiYellow": "#c19c00"
    },
    "workbench.activityBar.visible": false,
    "window.zoomLevel": 0,
    "workbench.settings.useSplitJSON": true,
    "markdown.preview.fontFamily": "Ricty",
    "markdown.preview.fontSize": 16,
    "markdown.preview.scrollEditorWithPreview": false,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.fontSize": 16,
    "files.autoGuessEncoding": true,
    "window.menuBarVisibility": "toggle",
    "breadcrumbs.enabled": false,
    "window.restoreWindows": "none",
    "workbench.enableExperiments": false,
    "npm.fetchOnlinePackageInfo": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "window.restoreFullscreen": true,
    "editor.glyphMargin": false,
    "workbench.editor.untitled.hint": "hidden",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "security.workspace.trust.untrustedFiles": "open",
    "telemetry.telemetryLevel": "off",
    "security.workspace.trust.enabled": false,
    // "[c]": {
    //     "editor.glyphMargin": true
    // },
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.autoClosingQuotes": "never",
    "C_Cpp.loggingLevel": "Information",
    "editor.parameterHints.enabled": false,
    "remote.SSH.remotePlatform": {
        "ao": "linux",
        "rt": "linux"
    },
    "[arduino]": {
        "arduino.path": "C:/Program Files (x86)/Arduino",
        "C_Cpp.intelliSenseEngine": "Tag Parser",
        "arduino.logLevel": "info",
        "arduino.commandPath": "arduino_debug.exe",
        "arduino.defaultBaudRate": 115200
    },
    "workbench.layoutControl.enabled": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.accessibilitySupport": "off",
    "editor.folding": false
}