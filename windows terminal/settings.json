{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": [
        {
            "command": {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "unbound",
            "keys": "alt+shift+minus"
        },
        {
            "command": "unbound",
            "keys": "alt+shift+plus"
        },
        {
            "command": "unbound",
            "keys": "ctrl+shift+pgup"
        },
        {
            "command": "unbound",
            "keys": "ctrl+shift+pgdn"
        },
        {
            "command": "unbound",
            "keys": "ctrl+shift+up"
        },
        {
            "command": "unbound",
            "keys": "ctrl+shift+down"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "down"
            },
            "keys": "ctrl+alt+shift+minus"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": {
                "action": "scrollDown"
            },
            "keys": "shift+down"
        },
        {
            "command": "scrollUpPage",
            "keys": "shift+pgup"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "right"
            },
            "keys": "ctrl+alt+shift+\\"
        },
        {
            "command": "scrollDownPage",
            "keys": "shift+pgdn"
        },
        {
            "command": {
                "action": "scrollUp"
            },
            "keys": "shift+up"
        }
    ],
    "alwaysShowTabs": true,
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{}",
    "disableAnimations": true,
    "experimental.rendering.software": false,
    "initialCols": 100,
    "newTabMenu": [
        {
            "type": "remainingProfiles"
        }
    ],
    "profiles": {
        "defaults": {
            "altGrAliasing": false,
            "antialiasingMode": "cleartype",
            "bellStyle": "none",
            "closeOnExit": "always",
            "colorScheme": "user3",
            "cursorShape": "underscore",
            "font": {
                "cellHeight": "1.1",
                "face": "Ricty ShinDiminished",
                "size": 13.0
            },
            "intenseTextStyle": "all",
            "padding": "2, 0, 2, 2",
            "scrollbarState": "hidden",
            "suppressApplicationTitle": true
        },
        "list": [
            {
