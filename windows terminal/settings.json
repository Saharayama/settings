                "hidden": false,
                "icon": "ms-appx:///ProfileIcons/pwsh.png",
                "name": "pwsh",
                "startingDirectory": "%USERPROFILE%"
            },
            {
                "commandline": "C:\\Program Files\\Python312\\python.exe",
                "guid": "{}",
                "hidden": false,
                "icon": "C:\\Program Files\\Python312\\Lib\\test\\imghdrdata\\python.png",
                "name": "python",
                "startingDirectory": "%USERPROFILE%"
            },
            {
                "guid": "{}",
                "hidden": true,
                "name": "Windows PowerShell"
            },
            {
                "guid": "{}",
                "hidden": true,
                "name": "Azure Cloud Shell",
                "source": "Windows.Terminal.Azure"
            }
        ]
